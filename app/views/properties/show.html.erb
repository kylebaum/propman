<div class="page-header">
  <h3>Property #<%= @property.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Description</dt>
      <dd><%= @property.description %></dd>

      <dt>Address</dt>
      <dd><%= @property.address %></dd>

      <dt>City</dt>
      <dd><%= @property.city_id %></dd>

      <dt>Neighborhood</dt>
      <dd><%= @property.neighborhood_id %></dd>

      <dt>Petpolicy</dt>
      <dd><%= @property.petpolicy_id %></dd>

      <dt>Price</dt>
      <dd><%= @property.price %></dd>

      <dt>Numberbed</dt>
      <dd><%= @property.numberbed %></dd>

      <dt>Numberbath</dt>
      <dd><%= @property.numberbath %></dd>

      <dt>Owner</dt>
      <dd><%= @property.owner_id %></dd>

      <dt>Manager</dt>
      <dd><%= @property.manager_id %></dd>

      <dt>Tenant</dt>
      <dd><%= @property.tenant_id %></dd>

      <dt>User</dt>
      <dd><%= @property.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/properties" class="btn btn-primary">
        Back
      </a>
      <a href="/properties/<%= @property.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_property/<%= @property.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
