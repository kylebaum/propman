<div class="page-header">
  <h1>
    Properties
    <a href="/properties/new" class="btn btn-lg btn-success">New Property</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Description</th>
        <th>Address</th>
        <th>City</th>
        <th>Neighborhood</th>
        <th>Petpolicy</th>
        <th>Price</th>
        <th>Numberbed</th>
        <th>Numberbath</th>
        <th>Owner</th>
        <th>Manager</th>
        <th>Tenant</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @properties.each do |property| %>
      <tr>
        <td><%= property.description %></td>
        <td><%= property.address %></td>
        <td><%= property.city_id %></td>
        <td><%= property.neighborhood_id %></td>
        <td><%= property.petpolicy_id %></td>
        <td><%= property.price %></td>
        <td><%= property.numberbed %></td>
        <td><%= property.numberbath %></td>
        <td><%= property.owner_id %></td>
        <td><%= property.manager_id %></td>
        <td><%= property.tenant_id %></td>
        <td><%= property.user_id %></td>
        <td>
          <a href="/properties/<%= property.id %>" class="btn btn-primary">Show</a>
          <a href="/properties/<%= property.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_property/<%= property.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
