<div class="page-header">
  <h1>
    Maintenancerequests
    <a href="/maintenancerequests/new" class="btn btn-lg btn-success">New Maintenancerequest</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Tenant</th>
        <th>Owner</th>
        <th>Manager</th>
        <th>Property</th>
        <th>Body</th>
        <th>Actions</th>
      </tr>

      <% @maintenancerequests.each do |maintenancerequest| %>
      <tr>
        <td><%= maintenancerequest.user_id %></td>
        <td><%= maintenancerequest.tenant_id %></td>
        <td><%= maintenancerequest.owner_id %></td>
        <td><%= maintenancerequest.manager_id %></td>
        <td><%= maintenancerequest.property_id %></td>
        <td><%= maintenancerequest.body %></td>
        <td>
          <a href="/maintenancerequests/<%= maintenancerequest.id %>" class="btn btn-primary">Show</a>
          <a href="/maintenancerequests/<%= maintenancerequest.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_maintenancerequest/<%= maintenancerequest.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
