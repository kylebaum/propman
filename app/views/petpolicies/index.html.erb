<div class="page-header">
  <h1>
    Petpolicies
    <a href="/petpolicies/new" class="btn btn-lg btn-success">New Petpolicy</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Policy</th>
        <th>Actions</th>
      </tr>

      <% @petpolicies.each do |petpolicy| %>
      <tr>
        <td><%= petpolicy.policy %></td>
        <td>
          <a href="/petpolicies/<%= petpolicy.id %>" class="btn btn-primary">Show</a>
          <a href="/petpolicies/<%= petpolicy.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_petpolicy/<%= petpolicy.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
